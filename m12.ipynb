   "source": [
    "import numpy as np\n",
    "from scipy.cluster.hierarchy import dendrogram,linkage\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "x=np.array([[1,2],[1,4],[1,0],[4,2],[4,4],[4,0]])\n",
    "\n",
    "z=linkage(x,'ward')\n",
    "\n",
    "dendrogram(z)\n",
    "\n",
    "plt.show()\n"
   ]
